public class Test {
	public static void main(String[]args) {
	   
		int result = factorial(4);//int result = FactorialTeest.factorial(4);
		System.out.println(result);
		 
	}//main문 끝
	static int factorial(int n) {
		int result =0;
		if(n == 1) {
			result = 1;
		}else {
			result = n * factorial(n-1);
		}
		return result;
	}
}
// 24
