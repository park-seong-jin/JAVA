import java.util.Scanner;

public class Test {
	public static void main(String[]args) {
		int num ;
		Scanner sc = new Scanner(System.in);
		System.out.print("*을 출력할 라인의 수를 입력하세요 >");
		num = sc.nextInt();
		
		for(int i = 1; i <=num; i ++) {
			for(int j = i ; j >0; j--) {
				System.out.print("*");
			}System.out.println();
		}
	}
}
